{
  "name": "helloworldui5",
  "version": "0.0.1",
  "description": "A Fiori Hello World App",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@openui5/types": "^1.133.0",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/types": "~1.133.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@ui5/cli": "^3.0.0",
    "@wdio/cli": "^8.43.0",
    "@wdio/globals": "^9.12.0",
    "@wdio/local-runner": "^8.43.0",
    "@wdio/mocha-framework": "^8.41.0",
    "@wdio/spec-reporter": "^8.43.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "ui5-task-zipper": "^3.1.3",
    "ui5-tooling-transpile": "^3.3.7",
    "wdio-geckodriver-service": "^5.0.2",
    "wdio-ui5-service": "^2.1.1"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#sapbtphelloworldui5-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#sapbtphelloworldui5-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "ts-typecheck": "tsc --noEmit",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy": "fiori cfDeploy",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "unit-tests": "fiori run --open 'test/unit/unitTests.qunit.html'",
    "int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "build:mta": "rimraf resources mta_archives && mbt build",
    "undeploy": "cf undeploy sapbtphelloworldui5 --delete-services --delete-service-keys --delete-service-brokers",
    "wdi5": "wdio run ./webapp/test/wdi5/wdio.conf.ts"
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
